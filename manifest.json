{
    "name": "Ido Vortaro Angla",
    "description": "",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icons": {
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>",
        "*://*/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [ "db/20210525_ido_min.json" ],
            "matches": ["<all_urls>"],
            "extension_ids": []
        }
    ],
     "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["contentScript.js"]
        }
    ]
}